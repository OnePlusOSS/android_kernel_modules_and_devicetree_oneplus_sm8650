	wsa884x_0221: wsa884x@02170221 {
		status = "disabled";
	};

	wsa883x_0221: wsa883x@02170221 {
		status = "okay";
	};

	wsa883x_0222: wsa883x@02170222 {
		status = "okay";
	};
};

